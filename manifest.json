{
  "name": "b2b-checkout-settings",
  "vendor": "vtex",
  "version": "1.0.2",
  "title": "B2B Checkout Settings",
  "description": "Adds B2B functionalities to the checkout",
  "builders": {
    "admin": "0.x",
    "checkout-ui-custom": "0.x",
    "docs": "0.x",
    "graphql": "1.x",
    "messages": "1.x",
    "node": "6.x",
    "react": "3.x",
    "vtex.storefront-permissions": "1.x"
  },
  "dependencies": {
    "vtex.graphql-server": "1.x",
    "vtex.styleguide": "9.x",
    "vtex.b2b-organizations-graphql": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "vtex.graphql-server:resolve-graphql"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "update-app-settings"
    },
    {
      "name": "SaveOrderFormConfiguration"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/checkout/pvt/configuration/orderForm"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
